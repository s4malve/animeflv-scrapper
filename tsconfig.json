{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "suppressImplicitAnyIndexErrors": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@icons/*": ["./src/components/icons/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@/*": ["./src/*"],
      "@css/*": ["./src/css/*"]
    }
  }
}
